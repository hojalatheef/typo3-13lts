## RSS Feed
## See: https://docs.typo3.org/p/georgringer/news/master/en-us/AdministratorManual/BestPractice/Rss/Index.html#rss-feed-configuration
plugin.tx_news {
  view {
    # cat=plugin.tx_news/file; type=string; label=Path to template root (FE)
    templateRootPath = EXT:jwsitepackage12/Resources/Private/Extensions/News/Templates/
    # cat=plugin.tx_news/file; type=string; label=Path to template partials (FE)
    partialRootPath = EXT:jwsitepackage12/Resources/Private/Extensions/News/Partials/
    # cat=plugin.tx_news/file; type=string; label=Path to template layouts (FE)
    layoutRootPath = EXT:jwsitepackage12/Resources/Private/Extensions/News/Layouts/
  }

  settings {
    # cat=plugin.tx_news/file; type=string; label=Path to CSS file
    cssFile =
  }
}