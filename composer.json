{
	"name": "typo3/cms-base-distribution",
	"description" : "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"type": "project",
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"platform": {
			"php": "8.2.0"
		},
		"sort-packages": true
	},
	"repositories": {
		"0": {
			"type": "path",
			"url": "./Source/Extension/*",
			"options": {
				"reference": "none",
				"versions": {
					"jweiland/jwsitepackage13": "1.0.0"
				}
			}
		}
	},
	"require": {
		"b13/container": "dev-master",
		"georgringer/news": "dev-12-13",
		"jweiland/jwsitepackage13": "^1.0",
		"typo3/cms-backend": "^13.3.0",
		"typo3/cms-belog": "^13.3.0",
		"typo3/cms-beuser": "^13.3.0",
		"typo3/cms-core": "^13.3.0",
		"typo3/cms-dashboard": "^13.3.0",
		"typo3/cms-extbase": "^13.3.0",
		"typo3/cms-extensionmanager": "^13.3.0",
		"typo3/cms-felogin": "^13.3.0",
		"typo3/cms-filelist": "^13.3.0",
		"typo3/cms-fluid": "^13.3.0",
		"typo3/cms-fluid-styled-content": "^13.3.0",
		"typo3/cms-form": "^13.3.0",
		"typo3/cms-frontend": "^13.3.0",
		"typo3/cms-impexp": "^13.3.0",
		"typo3/cms-info": "^13.3.0",
		"typo3/cms-install": "^13.3.0",
		"typo3/cms-reactions": "^13.3.0",
		"typo3/cms-rte-ckeditor": "^13.3.0",
		"typo3/cms-seo": "^13.3.0",
		"typo3/cms-setup": "^13.3.0",
		"typo3/cms-sys-note": "^13.3.0",
		"typo3/cms-t3editor": "^13.3.0",
		"typo3/cms-tstemplate": "^13.3.0",
		"typo3/cms-viewpage": "^13.3.0",
		"typo3/cms-webhooks": "^13.3.0"
	}
	,
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	}
}
